{
  "rules": {
    "@lightscript/no-unused-vars": 1,
    "no-undef": 1,
    "no-const-assign": 1,
    "@lightscript/variables": 1,
    "@lightscript/no-illegal-in-iife": 1,
    "@lightscript/no-implicit-imports": 1,
    "@lightscript/match-requires-else": 1,
    "@lightscript/no-unnecessary-const": 1,
    "@lightscript/no-unnecessary-semi": 1,
    "@lightscript/no-unnecessary-comma": 1
  }
}
